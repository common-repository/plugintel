msgid "Settings"
msgstr "Configuración"

# @ csa-plugintel
#: plugintel.php:255
msgid "Minimum Rating"
msgstr "Calificación mínima"

# @ csa-plugintel
#: plugintel.php:259
msgid ""
"Do not show plugins with ratings below this value.  60 = 3 stars, 100 = 5 "
"stars."
msgstr ""
"No mostrar los plugins con calificaciones menores a este valor. 60 = 3 "
"estrellas, 100 = 5 estrellas."

# @ csa-plugintel
#: plugintel.php:263
msgid "Maximum Rating"
msgstr "Calificación máxima"

# @ csa-plugintel
#: plugintel.php:267
msgid ""
"Do not show plugins with ratings above this value.  60 = 3 stars, 100 = 5 "
"stars."
msgstr ""
"No mostrar los plugins con calificaciones mayores a este valor. 60 = 3 "
"estrellas, 100 = 5 estrellas."

# @ csa-plugintel
#: plugintel.php:271
msgid "Min Number of Ratings"
msgstr "Número de calificaciones mínimo"

# @ csa-plugintel
#: plugintel.php:275
msgid "Do not show plugins with fewer than this number of ratings. Default: 2."
msgstr ""
"No mostrar los plugins con menos que este número de calificaciones. "
"Predeterminado:2."

# @ csa-plugintel
#: plugintel.php:279
msgid "Minimum Tested Version"
msgstr "Mínima Versión Probada"

# @ csa-plugintel
#: plugintel.php:283
msgid ""
"Do not show plugins that were not tested on this version of WordPress or  "
"higher. Default: 3.3."
msgstr ""
"No mostrar los plugins que no se probaron en esta versión de WordPress o "
"superior. Predeterminado: 3.3."

# @ csa-plugintel
#: plugintel.php:293
msgid ""
"Use these settings to filter out the plugins that are returned on the Add "
"New plugin page."
msgstr ""
"Use esta configuración para filtrar los plugins que son regresados en la "
"página de Añadir Nuevo Plugin."
